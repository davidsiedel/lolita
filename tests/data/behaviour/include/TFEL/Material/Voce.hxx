void additionalConvergenceChecks(bool& converged, const NumericType& error){
using namespace std;
using namespace tfel::math;
using std::vector;
const auto smt = this->stiffness_matrix_type;
if (converged) {
// checking status consistency
if (this->bpl) {
if (this->dp < -2*this->epsilon) {
// desactivating this system
converged = this->bpl = false;
}
} else {
const auto& s = this->sig;
const auto seq = sigmaeq(s);
const auto R_0 = this->ihr_Rinf_0+(this->ihr_R0_0-this->ihr_Rinf_0)*exp(-(this->ihr_b_0)*(this->p+(this->theta)*(this->dp)));
const auto R_1 = this->ihr_R0_1 + (this->ihr_H_1)*(this->p+(this->theta)*(this->dp));
const auto R = R_0 + R_1;
if(seq > R) {
converged = false;
this->bpl = true;
}
}
} // end of if(converged)

static_cast<void>(converged);
static_cast<void>(error);
static_cast<void>(smt);
} // end of additionalConvergenceChecks

