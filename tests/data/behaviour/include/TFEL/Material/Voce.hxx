computePartialJacobianInvert(const tfel::math::TinyPermutation<StensorSize+2>& jacobian_permutation, tfel::math::derivative_type<StrainStensor, StrainStensor>& partial_jacobian_eel)
{
using namespace tfel::math;
for(unsigned short idx=0;idx!=StensorSize;++idx){
tvector<StensorSize+2, NumericType> vect_e(NumericType(0));
vect_e(idx) = NumericType(1);
TinyMatrixSolve<StensorSize+2, NumericType>::back_substitute(this->jacobian,jacobian_permutation,vect_e);
